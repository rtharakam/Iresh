<Window x:Class="OInv.App.UserIntarfaces.ShowingData"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="ShowingData" Height="480" Width="800">
    <Grid>
        <Rectangle Fill="#FFF4F4F5" Stroke="Black">
            <Rectangle.Effect>
                <BlurEffect/>
            </Rectangle.Effect>
        </Rectangle>
        <Grid Margin="29.835,31.647,28.33,22.005"/>
        
        <DataGrid HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Height="429" Width="772">
            <DataGrid.Columns>
                <DataGridTextColumn Header="Item ID" Width="100" ></DataGridTextColumn>
                <DataGridTextColumn Header="Item Type" Width="150"></DataGridTextColumn>
            </DataGrid.Columns>
        </DataGrid>

    </Grid>
</Window>
